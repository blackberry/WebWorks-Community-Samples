{
	"formats": {
		"default": "#{streetAddress}\n#{postalCode} #{locality}\n#{country}",
		"nocountry": "#{streetAddress}\n#{postalCode} #{locality}"
	},
	"startAt": "end",
	"fields": [
		{ 
			"name": "locality",
			"pattern": "([0-9]{4} [A-Z]{2}\\s+)?([A-zÀÁÄÈÉËÌÍÏÒÓÖÙÚÜßàáäèéëìíòóöùúüÿŸ \\.\\-']+?)$",
			"line": "startAtLast",
			"matchGroup": 2
		},
		{
			"name": "postalCode",
			"line": "startAtLast",
			"pattern": "[0-9]{4}\\s+[A-Z]{2}"
		}
	]
}
