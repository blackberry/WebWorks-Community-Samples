{
	"formats": {
		"default": "#{streetAddress}\n#{locality}\n#{postalCode}\n#{country}",
		"nocountry": "#{streetAddress}\n#{locality}\n#{postalCode}"
	},
	"startAt": "end",
	"fields": [
		{
			"name": "postalCode",
			"line": "startAtLast",
			"pattern": "[0-9]{6}$",
			"matchGroup": 0
		},
		{
			"name": "locality",
			"line": "last",
			"pattern": "[\\w\\.\\-' ]+"
		}
	]
}
